#include<iostream>
using namespace std;
int main(){
    int m3,ss,dldc,edc,avg,num;
    cout<<"enter the value for m3:";
    cin>>m3;
    cout<<"enter the value for ss:";
    cin>>ss;
    cout<<"enter the value for :";
    cin>>dldc;
    cout<<"enter the value for edc:";
    cin>>edc;
    avg=(m3+ss+dldc+edc)/4;
    if(avg==90){
        cout<<"the grade is o:";
    
    }
    else if(avg<=100 && avg >= 80)
    {
        cout<<"the gade is A+ :";
    }
    else if(avg>=60 && avg< 80){
        cout<<"the grade is A :";
    }
    else if (avg<60)
    {
        cout<<"fail ahi tu lawde";
    }
}
